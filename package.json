    "next-tick": "^1.1.0",
    "node-version-use": "*",
    "on-one": "^1.0.0",
    "pinkie-promise": "*",
    "readable-stream": "^2.0.2",
    "ts-dev-stack": "*",
    "tsds-config": "*"
