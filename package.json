    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/fs-iterator.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
