{
  "name": "fs-iterator",
  "version": "3.0.1",
  "description": "A file system iterator with filter and asyncIterator iterafaces. Supports Node 0.10 and above",
  "main": "index.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "test": "mocha test/spec/**/*.test.js",
    "compat": "nvs 14,12,10,8,6,4,0.12,0.10,0.8 mocha-compat test/spec/**/*.test.js",
    "lint": "eslint .",
    "format": "prettier --write '**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/fs-iterator.git"
  },
  "keywords": [
    "recursive",
    "readdir",
    "fs",
    "filesystem",
    "filter",
    "iterator",
    "asyncIterator",
    "Symbol.asyncIterator"
  ],
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "license": "MIT",
  "homepage": "https://github.com/kmalakoff/fs-iterator",
  "dependencies": {
    "async-compat": "^1.4.5",
    "fifo": "^2.3.0",
    "fs-stats-spys": "^1.0.1",
    "fs.realpath": "^1.0.0",
    "maximize-iterator": "^2.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "end-of-stream": "^1.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-generate": "^1.8.0",
    "is-promise": "^4.0.0",
    "mocha": "^7.1.2",
    "mocha-compat": "^3.5.5",
    "next-tick": "^1.1.0",
    "nvs": "^0.1.8",
    "object.assign": "^4.1.0",
    "prettier": "^2.0.5",
    "rimraf2": "^0.1.0",
    "starts-with": "^1.0.2"
  }
}
