  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.6",
    "end-of-stream": "^1.4.4",
    "fs-generate": "^1.9.9",
    "fs-stats-spys": "^1.1.9",
    "is-promise": "^4.0.0",
    "next-tick": "^1.1.0",
    "pinkie-promise": "^2.0.1",
    "rimraf2": "^2.8.2",
    "starts-with": "^1.0.2"
